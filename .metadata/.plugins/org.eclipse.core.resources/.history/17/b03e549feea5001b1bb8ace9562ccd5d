package com.cursojava.aula37;

public class Teste {

	public static void main(String[] args) {
		
		Pessoa aluno = new Aluno();
		Pessoa professor = new Professor();
		Pessoa pessoa = new Pessoa();
		
		pessoa.setEndereco("Rua 1 n 1");
		aluno.setEndereco("Rua 2 n 2");
		professor.setEndereco("Rua 3 n 3");
		
		System.out.print(pessoa.obterEtiquetaEndereco());
		System.out.print(aluno.obterEtiquetaEndereco());
		System.out.print(professor.obterEtiquetaEndereco());
	}

}
