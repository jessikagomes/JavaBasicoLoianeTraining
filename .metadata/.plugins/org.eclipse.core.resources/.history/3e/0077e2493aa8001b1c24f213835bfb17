package com.cursojava.aula43exe;

public class Peixe extends Animal{
	
	private String caracteristicas = "Barbatanas e cauda";
	
	@Override
	public void setCor(String cor) {
		super.setCor("cinzenta");
	}
	@Override
	public void setAmbiente(String ambiente) {
		super.setAmbiente("Mar");
	}
	@Override
	public void setPatas(int patas) {
		super.setPatas(0);
	}
	
	public String getCaracteristicas() {
		return caracteristicas;
	}
	public void setCaracteristicas(String caracteristicas) {
		this.caracteristicas = caracteristicas;
	}
	public Peixe() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	public Peixe(String nome, int comprimento, int patas, String cor, String ambiente, double velocidade) {
		super(nome, comprimento, patas, cor, ambiente, velocidade);
	}
	@Override
	public String toString() {
		return "Peixe [caracteristicas=" + caracteristicas + ", getNome()=" + getNome() + ", getComprimento()="
				+ getComprimento() + ", getPatas()=" + getPatas() + ", getCor()=" + getCor() + ", getAmbiente()="
				+ getAmbiente() + ", getVelocidade()=" + getVelocidade() + "]";
	}
	
	
}
